<% if character.bouqueted_by?(current_user) %>
    	<%= link_to character_bouquet_path(character), method: :delete do %>
		<%= image_tag('flowers/brunfelsia_l.png', class: "cover", style: "height:60px;") %>
      <!-- <i class="fa fa-heart" aria-hidden="true" style="color: red;"></i> -->
      <p>
	  <%= character.bouquets.count %> ブーケ
	　</p>
    <% end %>
<% else %>
    <%= link_to "ブーケを投げる！" ,character_bouquets_path(character), class: "btn btn-primary", method: :post %>
<% end %>