<%= form_for(@character.active_partnerships.find_by(followed_id: partner.id), html: { method: :delete}, data: {confirm: "本当にパートナーを解消しますか？"}, remote: true) do |f| %>
	<%= hidden_field_tag :followed_id, partner.id %>
	<%= hidden_field_tag :follower_id, @character.id %>
	<%= f.submit "パートナー解消", class: "btn btn-danger" %>
<% end %>

