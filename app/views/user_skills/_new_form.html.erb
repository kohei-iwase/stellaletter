<%= form_with model:@skill,url: user_skills_path(@skill), local: true do |f| %>
	<div class="skill_form">
		<%#= attachment_image_tag @skill, :image,fallback: "characters/no_image.png"%>
        <p><%#= f.attachment_field :image, id:"attachmentImage"%></p>
		<%= f.label :スキル名 %>
		<%= f.text_field :name, class: "new_form" %>
		<%= f.submit '追加', class: "btn btn-primary" %>
	</div>
<% end %>